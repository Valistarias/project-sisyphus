{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react",
    "target": "esnext",
    "lib": ["ES2023", "dom", "dom.iterable"],
    "sourceMap": true,
    "strictNullChecks": true,
    "types": ["node", "vite-plugin-svgr/client"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowJs": true,
    "allowImportingTsExtensions": true
  },
  "include": ["./"]
}
